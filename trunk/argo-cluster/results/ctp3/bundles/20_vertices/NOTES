# aot is tagged incorrectly in raw files. Run the following
# to correct:

for b in igbundle1821410342283379275 igbundle2506244459619732059 igbundle5395938183197143755 igbundle5589653096263047498 igbundle862786164042876724; do

   for f in `grep -l ao3 raw/$b/*`; do
       sed "s/ao3/aot/" < $f > xxx
       mv xxx $f
   done
       
done

